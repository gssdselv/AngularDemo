                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        PJ.PJ_CODIGO, PJ.PA_CODIGO, PJ.PJ_DESCRIPTION, PJ.PU_USUARIO, PJ.PC_CODIGO, PJ.PL_CODIGO, PJ.PJ_OPEN_DATE, PJ.PJ_COMMIT_DATE, 
                         PJ.PJ_CLOSED_DATE, PJ.PS_CODIGO, ISNULL
                             ((SELECT        SUM(CASE WHEN TB_PT_PV_SAVINGTYPE.PV_DESCRIPTION &lt;&gt; 'Revenue' AND 
                                                          TB_PT_PV_SAVINGTYPE.PV_DESCRIPTION &lt;&gt; 'Planned Savings' THEN TB_PT_PO_COSTSAVING.PN_SAVINGAMOUNT ELSE 0 END)
                                 FROM            TB_PT_PO_COSTSAVING INNER JOIN
                                                          TB_PT_PV_SAVINGTYPE ON TB_PT_PV_SAVINGTYPE.PV_CODIGO = TB_PT_PO_COSTSAVING.PV_CODIGO
                                 WHERE        PJ_CODIGO = PJ.PJ_CODIGO), 0) AS PJ_COST_SAVING, isnull
                             ((SELECT        SUM(CASE WHEN TB_PT_PV_SAVINGTYPE.PV_DESCRIPTION = 'Revenue' THEN TB_PT_PO_COSTSAVING.PN_SAVINGAMOUNT ELSE 0 END)
                                 FROM            TB_PT_PO_COSTSAVING INNER JOIN
                                                          TB_PT_PV_SAVINGTYPE ON TB_PT_PV_SAVINGTYPE.PV_CODIGO = TB_PT_PO_COSTSAVING.PV_CODIGO
                                 WHERE        PJ_CODIGO = PJ.PJ_CODIGO), 0) AS PJ_REVENUE, isnull
                             ((SELECT        SUM(CASE WHEN TB_PT_PV_SAVINGTYPE.PV_DESCRIPTION = 'Planned Savings' THEN TB_PT_PO_COSTSAVING.PN_SAVINGAMOUNT ELSE 0 END)
                                 FROM            TB_PT_PO_COSTSAVING INNER JOIN
                                                          TB_PT_PV_SAVINGTYPE ON TB_PT_PV_SAVINGTYPE.PV_CODIGO = TB_PT_PO_COSTSAVING.PV_CODIGO
                                 WHERE        PJ_CODIGO = PJ.PJ_CODIGO), 0) AS PJ_PLAN_SAV, PJ.PJ_REMARKS, PJ.PJ_PERCENT_COMPLETION, dbo.TB_PT_PC_CUSTOMER.PC_DESCRICAO, 
                         dbo.TB_PT_PU_USERS.PU_NAME, dbo.TB_PT_PL_LOCATION.PL_DESCRICAO, dbo.TB_PT_PA_CATEGORY.PA_DESCRICAO, 
                         dbo.TB_PT_ST_STATUS.PS_DESCRICAO, PJ.PU_CRIADOR, TB_PT_PU_USERS_1.PU_NAME AS Expr1, PJ.PG_CODIGO, 
                         dbo.TB_PT_PG_SEGMENTOS.PG_DESCRICAO, PRE.PRE_CODIGO, PRE.PRE_DESCRICAO, PJ.PJ_EROOM, 
                         (CASE WHEN @UserViewrs = PJ.PU_CRIADOR THEN 1 ELSE
                             (SELECT DISTINCT COUNT(*)
                               FROM            ((SELECT        PU_USUARIO
                                                           FROM            TB_PT_PU_USERS
                                                           WHERE        PU_USUARIO = @UserViewrs AND PU_IS_ADMIN = 1)
                                                         UNION
                                                         (SELECT        PU_USUARIO
                                                          FROM            TB_PT_PJR_PROJECT_RESP_SEC
                                                          WHERE        PU_USUARIO = @UserViewrs AND PJ_CODIGO = PJ.PJ_CODIGO)) AS TB_CAN_DELETE) END) AS CAN_DELETE,
    (SELECT        COUNT(*) AS TOTAL
      FROM            dbo.TB_PT_PJA_PROJECT_ANEXOS
      WHERE        (PJ_CODIGO = PJ.PJ_CODIGO)) AS TOTAL_ATTACHMENTS,PJ_SITEBULEAD AS SiteLead
FROM            dbo.TB_PT_PJ_PROJECTS AS PJ INNER JOIN
                         dbo.TB_PT_PA_CATEGORY ON PJ.PA_CODIGO = dbo.TB_PT_PA_CATEGORY.PA_CODIGO INNER JOIN
                         dbo.TB_PT_PC_CUSTOMER ON PJ.PC_CODIGO = dbo.TB_PT_PC_CUSTOMER.PC_CODIGO LEFT OUTER JOIN
                         dbo.TB_PT_PRE_REGIONS AS PRE ON PJ.PRE_CODIGO = PRE.PRE_CODIGO INNER JOIN
                         dbo.TB_PT_PL_LOCATION ON PJ.PL_CODIGO = dbo.TB_PT_PL_LOCATION.PL_CODIGO INNER JOIN
                         dbo.TB_PT_PU_USERS ON PJ.PU_USUARIO = dbo.TB_PT_PU_USERS.PU_USUARIO INNER JOIN
                         dbo.TB_PT_ST_STATUS ON PJ.PS_CODIGO = dbo.TB_PT_ST_STATUS.PS_CODIGO INNER JOIN
                         dbo.TB_PT_PU_USERS AS TB_PT_PU_USERS_1 ON PJ.PU_CRIADOR = TB_PT_PU_USERS_1.PU_USUARIO INNER JOIN
                         dbo.TB_PT_PG_SEGMENTOS ON PJ.PG_CODIGO = dbo.TB_PT_PG_SEGMENTOS.PG_CODIGO
WHERE        (PJ.PJ_DESCRIPTION LIKE '%' + @Description + '%') AND (PJ.PU_USUARIO LIKE @Username) AND (PJ.PA_CODIGO LIKE @CategoryCode) AND 
                         (PJ.PS_CODIGO LIKE @StatusCode) AND (PJ.PC_CODIGO LIKE @CustomerCode) AND (PJ.PL_CODIGO LIKE @LocationCode) AND (CONVERT(VARCHAR(6), 
                         PJ.PJ_CODIGO) LIKE @ProjectCode) AND (ISNULL(CONVERT(VARCHAR(6), PRE.PRE_CODIGO), '%') LIKE @RegionCode) AND (PJ.PRE_CODIGO IN
                             (SELECT        PRE_CODIGO
                               FROM            dbo.SP_FIT_PT_GET_USER_REGIONS(@UserViewrs) AS SP_FIT_PT_GET_USER_REGIONS_1)) AND (@SavingCategoryCode = '%' OR
                         (PJ.PJ_CODIGO IN
                             (SELECT        dbo.TB_PT_PO_COSTSAVING.PJ_CODIGO
                               FROM            dbo.TB_PT_PO_COSTSAVING
                               WHERE        dbo.TB_PT_PO_COSTSAVING.PN_CODIGO LIKE @SavingCategoryCode))) AND (@StatusCode &lt;&gt; 20 OR
                         ((@StatusCode = 20) AND (PJ.PJ_CLOSED_DATE BETWEEN @StartDate AND DATEADD(s, - 1, DATEADD(d, 1, @EndDate)))))
